{
  "name": "SkonkworksAPI",
  "version": "0.1.0",
  "description": "seed for Digital Ventures APIs using Express",
  "author": "Justin Steranko",
  "license": "Proprietary",
  "scripts": {
    "serve": "NODE_ENV='development' nodemon --debug src/app.js --exec babel-node",
    "serve:development": "NODE_ENV='development' nodemon --debug src/app.js --exec babel-node",
    "serve:debug": "NODE_ENV='debug' nodemon --debug src/app.js --exec babel-node",
    "build": "babel ./src -d ./lib --copy-files",
    "start": "NODE_ENV='production' node ./lib/app.js",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "5.4.1"
  },
  "dependencies": {
    "app-module-path": "^1.1.0",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.13.3",
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.1",
    "http": "0.0.0",
    "mongoose": "^4.5.3",
    "nodemon": "^1.9.2",
    "passport": "^0.3.2"
  }
}
